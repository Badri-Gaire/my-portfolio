---
import Container from "../../shared/Container.astro";

interface Props {
  className?: string;
  id?: string;
  tagName?: string;
}

const { className = "", id, tagName = "section" } = Astro.props;
const Tag = tagName as any;
---

<Tag id={id} class={`py-5 ${className}`}>
  <Container>
    <slot />
  </Container>
</Tag>
